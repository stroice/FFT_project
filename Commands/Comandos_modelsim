%%Modelsim

--Importar el .v generado
--Cambiar en tb, stage
--simular ->cambiar el archivo de sdf      /tb_fft_1024_RAM_Mems_texto/dut

%%comandos

vcd files  /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max_PipeMem/FFT_Proposed.vcd 
vcd add -file  /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max_PipeMem/FFT_Proposed.vcd   -internal /tb_fft_1024_RAM_Mems_texto/dut/*
vcd add -file  /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max_PipeMem/FFT_Proposed.vcd  -in /tb_fft_1024_RAM_Mems_texto/dut/*
vcd add -file  /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max_PipeMem/FFT_Proposed.vcd  -out /tb_fft_1024_RAM_Mems_texto/dut/*
vcd on  /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max_PipeMem/FFT_Proposed.vcd
run

vcd flush /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max_PipeMem/FFT_Proposed.vcd




%%Linea de comandos normal


source /home/newuser/bash_vlsi

vcd2saif -input  /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max_PipeMem/FFT_Proposed.vcd -output /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max_PipeMem/FFT_Proposed.saif

%%Design compiler

set target_library			"/eda/technologies/tsmc_40nm/digital/Back_End/milkyway/tcbn40lpbwp_200a/frame_only_VHV_0d5_0/tcbn40lpbwp/LM/tcbn40lpbwptc.db"
set synthetic_library 		"dw_foundation.sldb"
set link_library 				"$target_library * $synthetic_library"

%%---Read ddc

%%Design compiler

read_saif -input /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max_PipeMem/FFT_Proposed.saif -instance tb_fft_1024_ram_mems_texto/dut

report_saif

uplevel #0 { report_power -analysis_effort low }

report_power -analysis_effort low > /home/mcre310/TFM/Main/scripts/reports/FFT_Proposed_max_PipeMem/FFT_Proposed_check.power_max.rpt





















%%Modelsim

--Importar el .v generado
--Cambiar en tb, stage
--simular ->cambiar el archivo de sdf      /tb_fft_1024_RAM_Mems_texto/dut

%%comandos

vcd files  /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max/FFT_Proposed.vcd 
vcd add -file  /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max/FFT_Proposed.vcd   -internal /tb_fft_1024_RAM_Mems_texto/dut/*
vcd add -file  /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max/FFT_Proposed.vcd  -in /tb_fft_1024_RAM_Mems_texto/dut/*
vcd add -file  /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max/FFT_Proposed.vcd  -out /tb_fft_1024_RAM_Mems_texto/dut/*
vcd on  /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max/FFT_Proposed.vcd
run

vcd flush /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max/FFT_Proposed.vcd




%%Linea de comandos normal


source /home/newuser/bash_vlsi

vcd2saif -input  /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max/FFT_Proposed.vcd -output /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max/FFT_Proposed.saif

%%Design compiler

set target_library			"/eda/technologies/tsmc_40nm/digital/Back_End/milkyway/tcbn40lpbwp_200a/frame_only_VHV_0d5_0/tcbn40lpbwp/LM/tcbn40lpbwptc.db"
set synthetic_library 		"dw_foundation.sldb"
set link_library 				"$target_library * $synthetic_library"

%%---Read ddc

%%Design compiler

read_saif -input /home/mcre310/TFM/Main/scripts/vcd/FFT_Proposed_max/FFT_Proposed.saif -instance tb_fft_1024_ram_mems_texto/dut

report_saif

uplevel #0 { report_power -analysis_effort low }

report_power -analysis_effort low > /home/mcre310/TFM/Main/scripts/reports/FFT_Proposed_max/FFT_Proposed_check.power_max.rpt






















%%Modelsim

--Importar el .v generado
--Cambiar en tb, stage
--simular ->cambiar el archivo de sdf      /tb_consumption_cordic/dut

%%comandos

vcd files  /home/mcre310/TFM/Main/scripts/CORDIC/vcd/CORDIC_max_freq/CORDIC_max_freq.vcd 
vcd add -file  /home/mcre310/TFM/Main/scripts/CORDIC/vcd/CORDIC_max_freq/CORDIC_max_freq.vcd   -internal /tb_consumption_cordic/dut/*
vcd add -file  /home/mcre310/TFM/Main/scripts/CORDIC/vcd/CORDIC_max_freq/CORDIC_max_freq.vcd  -in /tb_consumption_cordic/dut/*
vcd add -file  /home/mcre310/TFM/Main/scripts/CORDIC/vcd/CORDIC_max_freq/CORDIC_max_freq.vcd  -out /tb_consumption_cordic/dut/*
vcd on  /home/mcre310/TFM/Main/scripts/CORDIC/vcd/CORDIC_max_freq/CORDIC_max_freq.vcd

run

vcd flush /home/mcre310/TFM/Main/scripts/CORDIC/vcd/CORDIC_max_freq/CORDIC_max_freq.vcd




%%Linea de comandos normal


source /home/newuser/bash_vlsi

vcd2saif -input  /home/mcre310/TFM/Main/scripts/CORDIC/vcd/CORDIC_max_freq/CORDIC_max_freq.vcd -output /home/mcre310/TFM/Main/scripts/CORDIC/vcd/CORDIC_max_freq/CORDIC_max_freq.saif

%%Design compiler

set target_library			"/eda/technologies/tsmc_40nm/digital/Back_End/milkyway/tcbn40lpbwp_200a/frame_only_VHV_0d5_0/tcbn40lpbwp/LM/tcbn40lpbwptc.db"
set synthetic_library 		"dw_foundation.sldb"
set link_library 				"$target_library * $synthetic_library"

%%---Read ddc

%%Design compiler

read_saif -input /home/mcre310/TFM/Main/scripts/CORDIC/vcd/CORDIC_max_freq/CORDIC_max_freq.saif -instance tb_consumption_cordic/dut

report_saif

uplevel #0 { report_power -analysis_effort low }

report_power -analysis_effort low > /home/mcre310/TFM/Main/scripts/CORDIC/reports/CORDIC_max_freq/CORDIC_max_freq_check.power_max.rpt
